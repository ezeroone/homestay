<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <meta charset="utf-8">
        <title>Welcome to home stay srilanka</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="">
        @*<title>@ViewBag.Title</title>*@
        <!-- Load Google Web Font -->
        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>
        <!-- Load style.css: contains all css files concatenated to one single file -->
        <link href="~/content/css/style.css" rel="stylesheet">
        <link href="~/content/css/imperio.css" rel="stylesheet">
    
    </head>

    <body>
        @Html.Partial("_Header")
       
        @RenderBody()
     
        @Html.Partial("_Footer")

   </body>
        <script src="@Url.Content("~/Scripts/jquery-1.7.2.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("http://www.google.com/jsapi?autoload={'modules':[{name:'maps',version:3,other_params:'sensor=false'}]}")" type="text/javascript"></script>

        <script src="~/content/js/menu/jquery.ct.3LevelAccordion.min.js"></script>     		<!-- REQUIRED: Accordion Menu with filter-->
        <script src="~/content/js/slider/jquery.responsivethumbnailgallery.min.js"></script>  <!-- REQUIRED: Responsive Gallery Plugin -->
        <script src="~/content/js/slider/jquery.onebyone.min.js"></script>     				<!-- REQUIRED: Slider Plugin -->
        <script src="~/content/js/slider/jquery.touchwipe.min.js"></script>    				<!-- REQUIRED: Plugin to make Slider Plugin work on Touch Devices -->
           
        <script src="~/content/js/include/jquery.fitvids.min.js"></script>     		 		<!-- RECOMMENDED: Responsive videos -->			
        <script src="~/content/js/include/jquery.tweet.min.js"></script>     		 			<!-- OPTIONAL: Twitter display plugin -->
        <script src="~/content/js/include/jquery.equal-heights.min.js"></script> 	 			<!-- RECOMMENDED: Plugin to keep div heights consistant -->	
        <script src="~/content/js/include/jquery.todo.min.js"></script>					 	<!-- REQUIRED: Plugin to save "add to short list" items -->
        <script src="~/content/js/include/jquery.pubsub.min.js"></script>				 		<!-- REQUIRED: (If todo.js is in use) Dependent with todo.js -->
        <script src="~/content/js/include/jquery.select2.min.js"></script>			 		<!-- RECOMMENDED: Custom jQuery/searchable dropdowns -->	
        <script src="~/content/js/include/bootstrap.min.js"></script> 			 			<!-- REQUIRED: For BootStrap build -->
          
        <script src="~/content/js/config.js"></script>						 				<!-- DO NOT REMOVE: Contains major plugin initiations and functions -->
        
        <script src="~/content/js/jquery.dataTables.js"></script> 		
        <script src="~/content/js/tables.js"></script> 			 			
    
        @RenderSection("styles", required: false)

        @RenderSection("scripts", required: false)

   
</html>

